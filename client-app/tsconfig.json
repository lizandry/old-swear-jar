{
    "compilerOptions": {
      "sourceMap": true,
      "module": "commonjs",
      "target": "es5",
      "jsx": "react",
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "preserveConstEnums": true,
    "allowJs": true
    },
    "include": [
        "src"
    ],
    "exclude": [
      "node_modules/auth0-js"
  ],
    "types": [
        "node",
        "webpack",
        "react",
        "react-dom",
        "reactstrap"
    ],
    
    "compileOnSave": true,
    // "noImplicitAny": false
  }